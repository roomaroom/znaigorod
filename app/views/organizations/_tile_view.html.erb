<%= render partial: 'filters_and_search' %>

<div class="organization_wrapper">
  <%= render partial: 'promo_block' %>

  <div class='organization_content js-button-pagination'>
    <ul class='posters js-paginable-list' data-item-width="220">
      <% @presenter.clients.each do |item| %>
        <li class='item_organization fresh'>
          <%= render partial: 'suborganization', locals: { suborganization: item, width: 150, height: 150  } %>
        </li>
      <% end %>
    </ul>
  </div>
</div>

