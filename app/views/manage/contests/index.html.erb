<div class="new_link">
  <%= link_to t('new_contest'), new_manage_contest_url('contest[vfs_path]' => Contest.generate_vfs_path), :class => 'new_resource' %>
</div>

<div class="clearfix"></div>

<ul class='collection'>
  <% collection.each do |item| %>
    <li>
      <%= link_to item.title, [:manage, item] %>
      <%= item.is_a?(ContestVideo) ? 'Видеоконкурс' : 'Фотоконкурс' %>
    </li>
  <% end %>
</ul>
